{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Todo List App with React and Redux Toolkit",
  "type": "module",
  "homepage": "https://antcek.github.io/todo-list",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
